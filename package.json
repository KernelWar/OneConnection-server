{
  "name": "oneconnection-server",
  "version": "v0.3.4",
  "description": "Server controlador de Windows",
  "main": "index.js",
  "scripts": {
    "start": "copy \"environment\\environment.dev.js\" \"environment\\environment.js\" && electron .",
    "dist": "copy \"environment\\environment.prod.js\" \"environment\\environment.js\" && electron-builder",
    "build": "copy \"environment\\environment.prod.js\" \"environment\\environment.js\" && electron-builder build --win --publish never",
    "deploy": "copy \"environment\\environment.prod.js\" \"environment\\environment.js\" && electron-builder build --win --publish always"
  },
  "author": "KernelWar",
  "license": "GNU v3",
  "repository": {
    "type": "git",
    "url": "https://github.com/KernelWar/OneConnection-server.git"
  },
  "build": {
    "asar": true,
    "asarUnpack": [
      "config/",
      "node_modules/wmi-client"
    ],
    "productName": "OneConnection server",
    "win": {
      "icon": "logo.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "requestedExecutionLevel": "requireAdministrator",
      "publish": [
        {
          "provider": "github",
          "repo": "OneConnection-server",
          "owner": "KernelWar",
          "releaseType": "release"
        }
      ]
    },
    "nsis": {
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "oneClick": false,
      "perMachine": true,
      "license": "LICENSE",
      "deleteAppDataOnUninstall": true,
      "createDesktopShortcut": true,
      "differentialPackage": false
    }
  },
  "dependencies": {
    "battery-level": "^3.0.0",
    "bootstrap": "^4.6.0",
    "bootstrap-icons": "^1.5.0",
    "cors": "^2.8.5",
    "electron-log": "^4.4.1",
    "electron-updater": "^4.3.9",
    "express": "^4.17.1",
    "ffi-napi": "^3.1.0",
    "ip": "^1.1.5",
    "jquery": "^3.6.0",
    "mustache": "^4.2.0",
    "nircmd": "^2.0.0",
    "node-cmd": "^3.0.0",
    "node-disk-info": "^1.3.0",
    "os": "^0.1.2",
    "os-utils": "0.0.14",
    "qrcode": "^1.4.4",
    "ref-napi": "^2.1.2",
    "socket.io": "^2.4.1",
    "win-audio": "^2.0.2",
    "wmi-client": "^0.5.0"
  },
  "devDependencies": {
    "electron": "^12.2.1",
    "electron-builder": "^22.11.7"
  }
}
