{
  "name": "oneconnection-server",
  "version": "0.2.0",
  "description": "Server controlador de Windows",
  "main": "index.js",
  "scripts": {
    "start": "electromon .",
    "start-e": "electron .",    
    "dist": "electron-builder"
  },
  "author": "KernelWar",
  "license": "GNU v3",
  "build": {
    "asar": true,
    "asarUnpack": [
      "config/",
      "node_modules/wmi-client"
    ],
    "productName": "OneConnection server",
    "win": {
      "icon": "logo.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "oneClick": false,
      "perMachine": true,
      "license": "LICENSE"
    }
  },
  "dependencies": {
    "battery-level": "^3.0.0",
    "bootstrap": "^4.5.2",
    "bootstrap-icons": "^1.0.0",
    "cors": "^2.8.5",
    "electron-log": "^4.3.0",
    "express": "^4.17.1",
    "ffi-napi": "^3.0.1",
    "ip": "^1.1.5",
    "jquery": "^3.5.1",
    "mustache": "^4.0.1",
    "nircmd": "^2.0.0",
    "node-cmd": "^3.0.0",
    "node-disk-info": "^1.1.0",
    "os": "^0.1.1",
    "os-utils": "0.0.14",
    "qrcode": "^1.4.4",
    "ref-napi": "^2.1.2",
    "socket.io": "^2.3.0",
    "win-audio": "^2.0.2",
    "wmi-client": "^0.5.0"
  },
  "devDependencies": {
    "electromon": "^1.0.10",
    "electron": "^10.1.2",
    "electron-builder": "^22.8.1"
  }
}
